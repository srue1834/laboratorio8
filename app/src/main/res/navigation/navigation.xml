<?xml version="1.0" encoding="utf-8"?>

<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/inicio">

    <fragment
        android:id="@+id/inicio"
        android:name="com.example.laboratorio8.inicio.Inicio"
        android:label="fragment_inicio"
        tools:layout="@layout/fragment_inicio" >
        <action
            android:id="@+id/action_inicio_to_noticias"
            app:destination="@id/noticias" />
    </fragment>
    <fragment
        android:id="@+id/noticias"
        android:name="com.example.laboratorio8.noticias.Noticias"
        android:label="fragment_noticias"
        tools:layout="@layout/fragment_noticias" />
</navigation>